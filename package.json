    "prettier-check": "npx prettier --cache --check .",
    "serve": "npx http-server ./dist -o -c-1",
    "build": "npm run prettier && tsc --build"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fboes/aerofly-missions.git"
