{
  "name": "Aerofly Missionsgerät",
  "short_name": "Missionsgerät",
  "id": "aerofly-missionsgeraet",
  "description": "The Aerofly Missionsgerät converts simulator flight plan files for Aerofly FS 4, Microsoft Flight Simulator, X-Plane, GeoFS, and Garmin / Infinite Flight flight plan files.",
  "icons": [
    {
      "src": "./favicon-180x180.png",
      "sizes": "180x180",
      "type": "image/png"
    },
    {
      "src": "./favicon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "./favicon-mask-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "scope": "./",
  "display": "standalone",
  "orientation": "any",
  "start_url": "./",
  "dir": "ltr",
  "theme_color": "#1f6ed6",
  "background_color": "#1f6ed6",
  "lang": "en",
  "categories": ["simulation", "aviation", "weather", "tools", "utilities", "flight plan", "converter"],
  "version": "2.28.1",
  "screenshots": [],
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "application/octet-stream": [".mcf,.tmc,.fpl,.pln,.fms,.gpx,.cup"],
        "application/geo+json": [".geojson"],
        "application/json": [".json"]
      }
    }
  ]
}
